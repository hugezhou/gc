    <select id="getCountByBranchType" resultType="java.util.Map">
    select
        sum(case when u.branch_type =1 then 1 else 0 end) as jgCount,
        sum(case when u.branch_type =2 then 1 else 0 end) as jyCount,
        sum(case when u.branch_type =3 then 1 else 0 end) as ylCount,
        sum(case when u.branch_type =4 then 1 else 0 end) as ylyCount,
        sum(case when u.branch_type =5 then 1 else 0 end) as bgCount,
        sum(case when u.branch_type =6 then 1 else 0 end) as cyCount,
        sum(case when u.branch_type =7 then 1 else 0 end) as jdCount,
        sum(case when u.branch_type =8 then 1 else 0 end) as zzyCount,
        sum(case when u.branch_type =9 then 1 else 0 end) as cgCount,
        sum(case when u.branch_type =10 then 1 else 0 end) as jqCount,
        sum(case when u.branch_type =11 then 1 else 0 end) as jtCount,
        sum(case when u.branch_type =12 then 1 else 0 end) as scCount,
        sum(case when u.branch_type =13 then 1 else 0 end) as czfCount,
        sum(case when u.branch_type in (14,15,16,17,18,19,20,21,22) then 1 else 0 end) as jxCount,
        sum(case when u.branch_type =23 then 1 else 0 end) as shyCount,
        sum(case when u.branch_type =24 then 1 else 0 end) as gcCount,
        sum(case when u.branch_type =25 then 1 else 0 end) as gdCount,
        sum(case when u.branch_type =26 then 1 else 0 end) as whCount,
        sum(case when u.branch_type =27 then 1 else 0 end) as yrCount,
        sum(case when u.branch_type =28 then 1 else 0 end) as txCount
        from unit u
        <where>
        1=1
        and u.net_state = 1
        and u.expire_date is null
        <if test="selCtrlIdList != null and selCtrlIdList.size() > 0">
            and u.ctrl_center_id in
            <foreach collection="selCtrlIdList" item="item" index="index"
                     open="(" separator="," close=")">
                #{item}
            </foreach>
        </if>

    </where>
    </select>

