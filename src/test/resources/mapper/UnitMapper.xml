    <select id="findUnitListFromWeb" resultMap="BaseResultMap">
        SELECT u.* FROM UNIT u
        <if test="ctrlCenterBusyStr != null ">
            left join sys_user s on s.id = u.ctrl_center_busy_id
        </if>
        <where>
            1=1
            and u.expire_date is null
            <if test="ctrlCenterBusyStr != null">
                and s.real_name like concat(concat('%',#{ctrlCenterBusyStr}),'%')
            </if>
            <if test="netState != null">
                and u.NET_STATE = #{netState}
            </if>
            <if test="gatewayModelList != null and gatewayModelList.size() > 0">
                and u.id in (
                select distinct g.unit_id from
                gateway g
                where
                g.expire_date is null
                and g.model in
                <foreach collection="gatewayModelList" item="item" index="index"
                         open="(" separator="," close=")">
                    #{item}
                </foreach>
                )
            </if>
            <if test="name != null">
                and u.name like concat(concat('%',#{name}),'%')
            </if>

            <if test="hasFloor != null">
                and u.has_floor=#{hasFloor}
            </if>
            <if test="hasFireRoom != null">
                and u.has_fire_room=#{hasFireRoom}
            </if>

            <if test="linkPhone != null">
                and u.link_phone=#{linkPhone}
            </if>

            <if test="code != null">
                and u.code like concat(concat('%',#{code}),'%')
            </if>
            <if test="ctrlCenterTechId != null">
                and u.CTRL_CENTER_TECH_ID = #{ctrlCenterTechId}
            </if>
            <if test="ctrlCenterBusyId != null">
                and u.ctrl_center_busy_id = #{ctrlCenterBusyId}
            </if>
            <if test="unitIdList != null and unitIdList.size() > 0">
                and u.id in
                <foreach collection="unitIdList" item="item" index="index"
                         open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>

            <if test="selCtrlIdList != null and selCtrlIdList.size() > 0">
                and u.CTRL_CENTER_ID in
                <foreach collection="selCtrlIdList" item="item" index="index"
                         open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>
            <if test="ctrlCenterIdList != null and ctrlCenterIdList.size() > 0">
                and u.CTRL_CENTER_ID in
                <foreach collection="ctrlCenterIdList" item="item" index="index"
                         open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>
            order by u.create_date desc
        </where>
    </select>



    <select id="findUnitListBS" resultMap="BaseResultMap">
        SELECT u.lat,u.lng,u.name,u.code,u.LINK_PHONE FROM UNIT u
        <where>
        1=1
        and u.expire_date is null
            <if test="ctrlCenterIdList != null and ctrlCenterIdList.size() > 0">
            and u.CTRL_CENTER_ID in
            <foreach collection="ctrlCenterIdList" item="item" index="index"
                     open="(" separator="," close=")">
                #{item}
            </foreach>
            </if>
        </where>
    </select>

    <select id="findUnitCenterList" resultMap="BaseResultMap">
        SELECT u.* FROM UNIT u
        <where>
            1=1
            and u.expire_date is null
            <if test="netState != null">
                and u.NET_STATE = #{netState}
            </if>
            <if test="name != null">
                and u.name like concat(concat('%',#{name}),'%')
            </if>
            <!--<if test="ctrlCenterId != null">-->
            <!--and u.CTRL_CENTER_ID = #{ctrlCenterId}-->
            <!--</if>-->
            <if test="code != null">
                and u.code like concat(concat('%',#{code}),'%')
            </if>
            <if test="id != null">
                and u.id = #{id}
            </if>
            <if test="ctrlCenterTechId != null">
                and u.CTRL_CENTER_TECH_ID = #{ctrlCenterTechId}
            </if>

            <if test="ctrlCenterBusyId != null">
                and u.ctrl_center_busy_id = #{ctrlCenterBusyId}
            </if>
            <if test="weiBaoId != null">
                and u.WEIBAO_ID = #{weiBaoId}
            </if>
            <if test="unitIdList != null and unitIdList.size() > 0">
                and u.id in
                <foreach collection="unitIdList" item="item" index="index"
                         open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>
            <if test="ctrlCenterIdList != null and ctrlCenterIdList.size() > 0">
                and u.CTRL_CENTER_ID in
                <foreach collection="ctrlCenterIdList" item="item" index="index"
                         open="(" separator="," close=")">
                    #{item}
