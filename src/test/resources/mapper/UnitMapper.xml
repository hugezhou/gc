                </foreach>
            </if>
            <if test="searchType != null and searchType ==1 ">
                and  TO_DAYS(u.create_date) = TO_DAYS(NOW())
            </if>
            <if test="searchType != null and searchType ==2 ">
                and date_format(u.create_date,'%Y-%m')=date_format(now(),'%Y-%m')
                and week(u.create_date) = week(curdate())
            </if>
            <if test="searchType != null and searchType ==3 ">
                and date_format(u.create_date,'%Y-%m')=date_format(now(),'%Y-%m')
            </if>
            <if test="searchType != null and searchType ==4 ">
                and year(u.create_date) = year(curdate())
                and quarter( u.create_date ) = quarter( curdate())
            </if>
            <if test="searchType != null and searchType ==5 ">
                and year(u.create_date) = year(curdate())
            </if>
        </where>
    </select>

